{
	"Add h1": {
		"prefix": "!h1",
		"body": [
			"# $1"
		],
		"description": "add a h1 part in markdown"
	},
	"Add collapse part": {
		"prefix": "!collapse",
		"body": [
			"<details>",
			"<summary>$1</summary>",
			"<p>",
			"$2",
			"</p>",
			"</details>"
		],
		"description": "add a collapse part in markdown"
	},
	"Add head": {
		"prefix": "!head",
		"body": [
			"<div align='center'>",
			"",
			"# $1",
			"",
			"*$2*",
			"</div>",
			""
		],
		"description": "add meta info in markdown"
	},
	"Add react component table props": {
		"prefix": "!table-react-props",
		"body": [
			"|name|description|type|default|",
			"|:---:|:---:|:---:|:---|",
			"|$1|$2|$3|$4|",
			""
		],
		"description": "add react component table props"
	},
	"Add inside link": {
		"prefix": "!link",
		"body": [
			"[$1]($2)"
		],
		"description": "add inside link"
	},
	"Add outside link": {
		"prefix": "!link-outside",
		"body": [
			"[$1 ðŸ”—]($2)"
		],
		"description": "add outside link"
	},
	"Add image with caption": {
		"prefix": "!image-with-left-caption",
		"body": [
			"![$1]($2)",
			"<p style='color: #bfbfbf'>$1</p>"
		],
		"description": "add image"
	},
	"Add image with left caption": {
		"prefix": "!image-with-center-caption",
		"body": [
			"![$1]($2)",
			"<p align='center' style='color: #bfbfbf'>$1</p>"
		],
		"description": "add image"
	}
}