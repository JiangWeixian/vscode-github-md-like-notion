{
	"Add h1": {
		"prefix": "!h1",
		"body": [
			"# $1"
		],
		"description": "add a h1 part in markdown"
	},
	"Add collapse part": {
		"prefix": "!collapse",
		"body": [
			"<details>",
			"<summary>$1</summary>",
			"<p>",
			"$2",
			"</p>",
			"</details>"
		],
		"description": "add a collapse part in markdown"
	},
	"Add head": {
		"prefix": "!head",
		"body": [
			"<div align='center'>",
			"",
			"# $1",
			"",
			"*$2*",
			"</div>",
			""
		],
		"description": "add meta info in markdown"
	},
	"Add react component table props": {
		"prefix": "!table-react-props",
		"body": [
			"|name|description|type|default|",
			"|:---:|:---:|:---:|:---|",
			"|$1|$2|$3|$4|",
			""
		],
		"description": "add react component table props"
	},
	"Add inside link": {
		"prefix": "!link",
		"body": [
			"[$1]($2)"
		],
		"description": "add inside link"
	},
	"Add outside link": {
		"prefix": "!link-outside",
		"body": [
			"[$1 ðŸ”—]($2)"
		],
		"description": "add outside link"
	},
	"Add image with caption": {
		"prefix": "!image-with-caption",
		"body": [
			"![$1]($2)*$1*"
		],
		"description": "add image with caption"
	},
	"Add image with center caption": {
		"prefix": "!image-with-center-caption",
		"body": [
			"<div align='center'>",
			"",
			"![$1]($2)",
			"*$1*",
			"",
			"</div>"
		],
		"description": "add image with center caption"
	},
	"Add todo list": {
		"prefix": "!todo-list",
		"body": [
			"- [ ] $1",
			"- [ ] $2"
		],
		"description": "add image"
	},
	"Add todo undo item": {
		"prefix": "!todo-undo-item",
		"body": [
			"- [ ] $1"
		],
		"description": "add image"
	},
	"Add todo done item": {
		"prefix": "!todo-done-item",
		"body": [
			"- [x] $2"
		],
		"description": "add image"
	}
}