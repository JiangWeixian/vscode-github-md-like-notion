{
	"Add collapse part": {
		"prefix": "!collapse",
		"body": [
			"<details>",
			"<summary>${1:summary}</summary>",
			"<p>",
			"${2:description}",
			"</p>",
			"</details>"
		],
		"description": "add a collapse part in markdown"
	},
	"Add simple repo description": {
		"prefix": "!description",
		"body": [
			"<div align='center'>",
			"",
			"# ${1:reponame}",
			"",
			"*${2:description}*",
			"",
			"</div>",
			""
		],
		"description": "add simple desciption for github-repo"
	},
	"Add repo description with logo": {
		"prefix": "!description-with-logo",
		"body": [
			"<div align='center'>",
			"",
			"![logo]($1)",
			"",
			"*${2:description}*",
			"",
			"</div>",
			""
		],
		"description": "add simple desciption with center logo for github-repo"
	},
	"Add repo description with righ logo": {
		"prefix": "!description-with-right-logo",
		"body": [
			"<img align='right' width='100' src='$1' alt='logo' />",
			"",
			"${2:description}",
			""
		],
		"description": "add simple desciption with right logo for github-repo"
	},
