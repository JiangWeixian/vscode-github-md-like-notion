{
	"Add h1": {
		"prefix": "!h1",
		"body": [
			"# $1"
		],
		"description": "add a h1 part in markdown"
	},
	"Add collapse part": {
		"prefix": "!collapse",
		"body": [
			"<details>",
			"<summary>$1</summary>",
			"<p>",
			"$2",
			"</p>",
			"</details>"
		],
		"description": "add a collapse part in markdown"
	},
	"Add head": {
		"prefix": "!head",
		"body": [
			"<div align='center'>",
			"",
			"# $1",
			"",
			"*$2*",
			"</div>",
			""
		],
		"description": "add meta info in markdown"
	},
